SINTECK-XT-SNMP-MIB DEFINITIONS ::= BEGIN

--
-- SINTECK XT MIB module
--

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, enterprises
        FROM SNMPv2-SMI

    OBJECT-GROUP, MODULE-COMPLIANCE
        FROM SNMPv2-CONF

    Utf8String
        FROM SYSAPPL-MIB;

sinteckXTMIB MODULE-IDENTITY
    LAST-UPDATED "202301300000Z"
    ORGANIZATION "www.sinteck.com.br"
    CONTACT-INFO    
	"postal:   Rua dos Bolivianos, 578
                    Sao Paulo - SP
                    Brazil

          email:    fabio@sinteck.com.br"
    DESCRIPTION
	"Sinteck XT SNMP MIB"
	
	REVISION     "202301300000Z"
    DESCRIPTION
	"Change OID value"

    REVISION     "202111160000Z"
    DESCRIPTION
	"New objects to change preset, current exciter, exciter 
	input and exciter audio channel readings."

    REVISION     "202001010000Z"
    DESCRIPTION
	"First version"

    ::= { enterprises 54532}

--
--  Net-SNMP enterprise-specific management objects
--
sinteckCpuObjects OBJECT IDENTIFIER ::= { sinteckXTMIB 6 }

sinteckXTConformance    OBJECT IDENTIFIER ::= { sinteckXTMIB 3 }
sinteckXTCompliances    OBJECT IDENTIFIER ::= { sinteckXTConformance 1 }
xtGroups         OBJECT IDENTIFIER ::= { sinteckXTConformance 2 }

sinteckXTCompliance     MODULE-COMPLIANCE
    STATUS          current
    DESCRIPTION
        "The compliance statement for SNMP entities which
         implement the SINTECK-XT-SNMP MIB"

    MODULE SINTECK-XT-SNMP-MIB
          MANDATORY-GROUPS { xtGroup }

    GROUP xtGroup
          DESCRIPTION
              "The xtGroup is mandatory, but no write access
               to objects is required for compliance."

    ::= { sinteckXTCompliances 1 }

xtGroup OBJECT-GROUP
    OBJECTS { 
        ForwardPower,
        ReflectedPower,
        RfInPower,
        IntAvgTemperature,
        Status,
        BatteryStatus,
        MainInputStatus,
        StationName,
        SationFrequency,
        PowerState,
        Errors,
        SelectedPreset,
        ExciterInput,
        ExciterAudioLeft,
        ExciterAudioRight,
        ExciterAudioMpx,
        AlarmMPX
    }
    STATUS  current
    DESCRIPTION
            "A collection of objects providing basic instrumentation and
            control of an SNMPv2 entity."
    ::= { xtGroups 1 }
    
ForwardPower OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Forward Power from XT module"
    ::= { sinteckCpuObjects 1 }
    

ReflectedPower OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Reflected Power from XT module"
    ::= { sinteckCpuObjects 2 }
    
RfInPower OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "RF IN Power from XT module"
    ::= { sinteckCpuObjects 3 }
    
IntAvgTemperature OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Internal Temperature from XT module"
    ::= { sinteckCpuObjects 4 }
    
Status OBJECT-TYPE
    SYNTAX Utf8String
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Current Status"
    ::= { sinteckCpuObjects 5 }
    
BatteryStatus OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Battery Status"
    ::= { sinteckCpuObjects 6 }
    
MainRStatus OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage AC R Status"
    ::= { sinteckCpuObjects 7 }

MainSStatus OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage AC S Status"
    ::= { sinteckCpuObjects 17 }

MainTStatus OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage AC T Status"
    ::= { sinteckCpuObjects 18 }
    
StationName OBJECT-TYPE
    SYNTAX Utf8String
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Station Name"
    ::= { sinteckCpuObjects 8 }
    
SationFrequency OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Station Frequency"
    ::= { sinteckCpuObjects 9 }
    
PowerState OBJECT-TYPE
    SYNTAX INTEGER {
        poweredoff(0),
		poweredon(1),
        safemode(2),
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Current Power State"
    ::= { sinteckCpuObjects 10 }
    
Errors OBJECT-TYPE
    SYNTAX Utf8String
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Error List"
    ::= { sinteckCpuObjects 11 }
    
SelectedPreset OBJECT-TYPE
    SYNTAX INTEGER {
        preset1(1),
        preset2(2),
        preset3(3),
        preset4(4),
        preset5(5),
        preset6(6)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Current Preset"
    ::= { sinteckCpuObjects 12 }

ExciterInput OBJECT-TYPE
    SYNTAX INTEGER {
        internal(0),
        external(1),
        usb(2),
        aes(3)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Exciter Input"
    ::= { sinteckCpuObjects 13 }
    
ExciterAudioLeft OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Exciter Audio Left"
    ::= { sinteckCpuObjects 14 }

ExciterAudioRight OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Exciter Audio Right"
    ::= { sinteckCpuObjects 15 }

ExciterAudioMpx OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Exciter Audio MPX"
    ::= { sinteckCpuObjects 16 }

ServiceMode OBJECT-TYPE
    SYNTAX Integer32 {
        Automatic(0),
        Automatic+Debug(1),
        Manual(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Mode Service"
    ::= { sinteckCpuObjects 19 }
    
OffsetBat OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Offset IBAT"
    ::= { sinteckCpuObjects 20 }
    
AlarmMPX OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Modulation MPX Alarm"
    ::= { sinteckCpuObjects 21 }    

END
    
